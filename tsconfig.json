{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["es2021"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@lib*": ["lib/*"]
    },
    "outDir": "./js",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
