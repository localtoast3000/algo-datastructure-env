{
  "name": "algo-env",
  "version": "1.0.0",
  "description": "Environment for running algorithms with TypeScript",
  "main": "index.ts",
  "scripts": {
    "compile": "npx tsc",
    "js-start": "nodemon main.js",
    "start": "tsnd --respawn index.ts",
    "algo": "tsnd --respawn algorithms/algo.ts",
    "new-algo": "node ./lib/new_algo/newAlgo.js",
    "data-s": "tsnd --respawn data_structures/data-s.ts"
  },
  "author": "Craig Chick",
  "license": "ISC",
  "dependencies": {
    "nodemon": "^2.0.20",
    "uid2": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
